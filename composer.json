{
    "name": "youandmedigital/craft-foxycarthmac",
    "description": "FoxyCart HMAC is a cryptographic method to prevent people from tampering with your product links",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "foxycart-hmac"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "You & Me Digital",
            "homepage": "https://youandme.digital"
        }
    ],
    "support": {
        "docs": "https://github.com/youandmedigital/craft-foxycarthmac/blob/master/README.md",
        "issues": "https://github.com/youandmedigital/craft-foxycarthmac/issues"
    },
    "require": {
        "craftcms/cms": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
          "youandmedigital\\foxycarthmac\\": "src/"
        }
    },
    "extra": {
        "name": "FoxyCart HMAC",
        "handle": "foxycarthmac",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/youandmedigital/craft-foxycarthmac/master/CHANGELOG.md",
        "components": {
            "foxyCartHMACService": "youandmedigital\\foxycarthmac\\services\\FoxyCartHMACService"
        },
        "class": "youandmedigital\\foxycarthmac\\FoxyCartHMAC"
    }
}
